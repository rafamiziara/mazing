{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "@rocketseat/eslint-config/next"
  ],
  "rules": {
		"prettier/prettier": ["error", {
      "printWidth": 140,
      "tabWidth": 2,
      "singleQuote": true,
      "arrowParens": "always",
      "trailingComma": "es5",
      "semi": false,
      "endOfLine": "auto"
    }]
	}
}
